  "private": true,
  "version": "0.4.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build --assetsDir bin",
    "preview": "vite preview",
    "deploy": "deploy.sh",
    "fetch": "node --experimental-modules --experimental-json-modules  ./buildtools/wiki_rip.mjs > ./buildtools/data_fetch_results.log",
    "data_fetch_validate": "node --experimental-modules ./buildtools/data_fetch_validate.mjs > ./buildtools/data_fetch_validate_results.log",
