  "dependencies": {
    "numeral": "^2.0.6",
    "vue": "^3.2.31",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "lua2js": "0.0.11",
    "luaparse": "^0.3.1",
    "@types/json5": "^2.2.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/compiler-sfc": "^3.2.31",
    "cheerio": "^1.0.0-rc.11",
    "he": "^1.2.0",
    "jest": "^28.1.0",
    "jsdom": "^20.0.0",
    "json5": "^2.2.1",
    "moo": "^0.5.1",
    "node-fetch": "^3.2.5",
    "parenthesis": "^3.1.8",
    "typescript": "^4.7.4",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.7"
  },
  "jest": {
    "transform": {}
  }
